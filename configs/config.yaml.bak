system:
  log:
    level: info
    path: /Users/log

host:
  spBlogAPIHost: 8080

database:
  mysql:
    master:
      host: localhost
      port: 3306
      username: username
      password: password
      dbName: dbName
      conn:
        maxIdle: 10
        maxOpen: 100
        maxLifetime: 600

gin:
  # mode: debug、release、test
  mode: debug

gorm:
  log:
    # logMode: silent、error、warn、info, default silent
    mode: silent

validator:
  # 設定錯誤訊息文字語言
  locale: zh
